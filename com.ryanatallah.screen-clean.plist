<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.ryanatallah.screen-clean</string>
  <key>StandardOutPath</key>
  <string>/Users/{USER_NAME}/Library/Logs/com.ryanatallah.screen-clean.log</string>
  <key>StandardErrorPath</key>
  <string>/Users/{USER_NAME}/Library/Logs/com.ryanatallah.screen-clean.log</string>
  <key>ProgramArguments</key>
  <array>
    <string>sh</string>
    <string>-c</string>
    <string>/usr/local/bin/screen-clean</string>
  </array>
  <key>RunAtLoad</key>
  <true/>
  <key>StartInterval</key>
  <integer>900</integer>
</dict>
</plist>

